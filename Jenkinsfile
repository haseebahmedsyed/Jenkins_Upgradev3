pipeline {
  agent any
  stages{
    stage('Init'){
      steps{
        echo 'This is Init phase'
      }
    }
    stage('Build'){
      steps{
        echo 'This is Build phase'
      }
    }
    stage('Deploy'){
      steps{
        echo 'This is Deploy phase'
      }
    }
    stage('Deploy Production'){
      steps{
        echo 'This is Deploy Production phase'
      }
    }
  }
}
